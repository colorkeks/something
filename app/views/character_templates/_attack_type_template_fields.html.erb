<%= f.error_notification %>
<tr class="fields item">
  <td><%= f.input :name, input_html: {class: 'form-control'} %></td>
  <td><%= f.input :attack, input_html: {class: 'form-control'} %></td>
  <td><%= f.input :attack_type, label: 'Attack_type', input_html: {class: 'form-control'} %></td>
  <td class="remove">
    <%= link_to_remove_fields '<i class="glyphicon glyphicon-remove"></i>', f, 'btn btn-bit btn-lg red' %>
  </td>
  <%= f.input :character_template_id, as: :hidden %>
</tr>