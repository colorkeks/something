<p id="notice"><%= notice %></p>

<h1>Listing Weekness Templates</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Percent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weekness_templates.each do |weekness_template| %>
      <tr>
        <td><%= weekness_template.type %></td>
        <td><%= weekness_template.percent %></td>
        <td><%= link_to 'Show', weekness_template %></td>
        <td><%= link_to 'Edit', edit_weekness_template_path(weekness_template) %></td>
        <td><%= link_to 'Destroy', weekness_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weekness template', new_weekness_template_path %>
