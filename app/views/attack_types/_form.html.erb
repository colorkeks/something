<%= simple_form_for(@attack_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :attack %>
    <%= f.input :crit_chance %>
    <%= f.input :crit_dmg %>
    <%= f.input :character_id %>
    <%= f.input :item_id %>
    <%= f.input :attack_type_template_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
