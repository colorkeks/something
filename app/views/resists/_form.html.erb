<%= simple_form_for(@resist) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :physical %>
    <%= f.input :fire %>
    <%= f.input :water %>
    <%= f.input :earth %>
    <%= f.input :holy %>
    <%= f.input :necrotic %>
    <%= f.input :nature %>
    <%= f.input :character_id %>
    <%= f.input :resist_template_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
